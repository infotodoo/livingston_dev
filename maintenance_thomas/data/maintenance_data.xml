<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Standard stages for Maintenance Request -->
        <record id="stage_5" model="maintenance.stage">
            <field name="name">NEW REQUEST</field>
            <field name="sequence">5</field>  
        </record>
        <record id="stage_6" model="maintenance.stage">
            <field name="name">IN PLANNING</field>
            <field name="sequence">6</field>
        </record>
        <record id="stage_7" model="maintenance.stage">
            <field name="name">PROGRAMMED</field>
            <field name="sequence">7</field>
        </record>
        <record id="stage_8" model="maintenance.stage">
            <field name="name">FINALIZED</field>
            <field name="fold" eval="False" />
            <field name="sequence">8</field>
        </record>
        <record id="stage_9" model="maintenance.stage">
            <field name="name">CANCELED WITH NOVELTY</field>
            <field name="sequence">9</field>
        </record>
        <record id="stage_10" model="maintenance.stage">
            <field name="name">QUALITY</field>
            <field name="sequence">10</field>
            <field name="done" eval="True" />
        </record>

    </data>
</odoo>
