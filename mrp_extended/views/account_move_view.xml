<?xml version="1.0"?>
<odoo>
    <data>
        <record id="mrp_prelimit_field" model="ir.ui.view">
            <field name="name">mrp.prelimit.field</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='to_check']" position="after">
                    <field name="prelimit_id" options="{'no_create': True, 'no_create_edit':True}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
