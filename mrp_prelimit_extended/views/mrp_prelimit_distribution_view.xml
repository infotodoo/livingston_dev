<odoo>
    <data>
        <record id="mrp_prelimit_distribution_view_tree" model="ir.ui.view">
            <field name="name">mrp.prelimit.distribution.tree</field>
            <field name="model">mrp.prelimit.distribution</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                   <sheet>
                    <field name="first_account_id"/>
                    <field name="second_account_id"/>
                    <field name="extra_account1"/>
                    <field name="extra_account2"/>
                    <field name="extra_account3"/>
                   </sheet>
                </tree>
            </field>
        </record>
        <record model="ir.actions.act_window" id="action_mrp_prelimit_distribution_tree">
            <field name="name"> Prelimit Distribution</field>
            <field name="res_model">mrp.prelimit.distribution</field>
            <field name="view_id" ref="mrp_prelimit_distribution_view_tree"/>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>
